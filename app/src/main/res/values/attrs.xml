<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="custume_ripple_imageview">
        <attr name="show_spacing_time" format="integer"></attr>
        <attr name="imageViewWidth" format="dimension"></attr>
        <attr name="imageViewHeight" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="my_ripple_view">
        <attr name="view_time" format="integer"/>
